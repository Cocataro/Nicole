{
  "env": {
    "GEMINI_API_KEY": "${GEMINI_API_KEY}",
    "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
    "DISCORD_BOT_TOKEN": "${DISCORD_BOT_TOKEN}"
  },
  "models": {
    "providers": {
      "ollama": {
        "baseUrl": "${OLLAMA_BASE_URL}",
        "apiKey": "ollama-local",
        "api": "openai-completions",
        "models": [
          {
            "id": "qwen2.5:14b",
            "name": "Qwen 2.5 14B",
            "reasoning": false,
            "input": [
              "text"
            ],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 32768,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-haiku-4-5",
        "fallbacks": [
          "ollama/qwen2.5:14b"
        ]
      },
      "workspace": "${OPENCLAW_HOME}/workspace",
      "compaction": {
        "mode": "safeguard"
      },
      "heartbeat": {
        "every": "0m",
        "model": "anthropic/claude-haiku-4-5"
      },
      "maxConcurrent": 1,
      "subagents": {
        "maxConcurrent": 1
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "name": "Nicole",
        "workspace": "${OPENCLAW_HOME}/workspace",
        "agentDir": "${OPENCLAW_HOME}/agents/main/agent",
        "identity": {
          "name": "Nicole",
          "theme": "Mission commander and chief of staff",
          "emoji": "\ud83c\udfaf"
        },
        "subagents": {
          "allowAgents": [
            "aria",
            "reed",
            "sage",
            "dylon",
            "hana",
            "max",
            "leo",
            "zara",
            "kai"
          ]
        }
      },
      {
        "id": "aria",
        "name": "Aria",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/aria",
        "agentDir": "${OPENCLAW_HOME}/agents/aria/agent",
        "model": "ollama/qwen2.5:14b",
        "identity": {
          "name": "Aria",
          "theme": "Deep research specialist",
          "emoji": "\ud83d\udd0e"
        }
      },
      {
        "id": "reed",
        "name": "Reed",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/reed",
        "agentDir": "${OPENCLAW_HOME}/agents/reed/agent",
        "model": "ollama/qwen2.5:14b",
        "identity": {
          "name": "Reed",
          "theme": "Real-time news scanner",
          "emoji": "\ud83d\udce1"
        }
      },
      {
        "id": "sage",
        "name": "Sage",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/sage",
        "agentDir": "${OPENCLAW_HOME}/agents/sage/agent",
        "model": "ollama/qwen2.5:14b",
        "identity": {
          "name": "Sage",
          "theme": "Social sentiment analyst",
          "emoji": "\ud83d\udcac"
        }
      },
      {
        "id": "dylon",
        "name": "Dylon",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/dylon",
        "agentDir": "${OPENCLAW_HOME}/agents/dylon/agent",
        "model": "ollama/qwen2.5:14b",
        "identity": {
          "name": "Dylon",
          "emoji": "\ud83d\udee1\ufe0f",
          "theme": "Risk management agent"
        },
        "subagents": {
          "allowAgents": []
        }
      },
      {
        "id": "hana",
        "name": "Hana",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/hana",
        "agentDir": "${OPENCLAW_HOME}/agents/hana/agent",
        "model": "google/gemini-3-flash-preview",
        "identity": {
          "name": "Hana",
          "emoji": "\ud83d\udcca",
          "theme": "Backtesting and strategy analysis specialist"
        },
        "subagents": {
          "allowAgents": []
        }
      },
      {
        "id": "max",
        "name": "Max",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/max",
        "agentDir": "${OPENCLAW_HOME}/agents/max/agent",
        "model": "ollama/qwen2.5:14b",
        "identity": {
          "name": "Max",
          "theme": "The Sniper — breakout trading specialist",
          "emoji": "\ud83d\udcc8"
        },
        "subagents": {
          "allowAgents": []
        }
      },
      {
        "id": "leo",
        "name": "Leo",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/leo",
        "agentDir": "${OPENCLAW_HOME}/agents/leo/agent",
        "model": "ollama/qwen2.5:14b",
        "identity": {
          "name": "Leo",
          "theme": "EMA Crossover — trend following specialist",
          "emoji": "\u2197\ufe0f"
        },
        "subagents": {
          "allowAgents": []
        }
      },
      {
        "id": "zara",
        "name": "Zara",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/zara",
        "agentDir": "${OPENCLAW_HOME}/agents/zara/agent",
        "model": "ollama/qwen2.5:14b",
        "identity": {
          "name": "Zara",
          "theme": "RSI Reversal — oversold dip buying specialist",
          "emoji": "\ud83d\udd04"
        },
        "subagents": {
          "allowAgents": []
        }
      },
      {
        "id": "kai",
        "name": "Kai",
        "workspace": "${OPENCLAW_HOME}/workspace/agents/kai",
        "agentDir": "${OPENCLAW_HOME}/agents/kai/agent",
        "model": "ollama/qwen2.5:14b",
        "identity": {
          "name": "Kai",
          "theme": "VWAP Reversion — intraday mean reversion specialist",
          "emoji": "\ud83c\udfaf"
        },
        "subagents": {
          "allowAgents": []
        }
      }
    ]
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "botToken": "${TELEGRAM_BOT_TOKEN}",
      "groupPolicy": "allowlist",
      "streamMode": "partial"
    },
    "discord": {
      "enabled": true,
      "groupPolicy": "allowlist"
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "${GATEWAY_AUTH_TOKEN}"
    },
    "tailscale": {
      "mode": "off",
      "resetOnExit": false
    },
    "nodes": {
      "denyCommands": [
        "camera.snap",
        "camera.clip",
        "screen.record",
        "calendar.add",
        "contacts.add",
        "reminders.add"
      ]
    }
  },
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      },
      "discord": {
        "enabled": true
      }
    }
  }
}
